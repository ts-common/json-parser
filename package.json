{
  "name": "@ts-common/json-parser",
  "version": "0.8.0",
  "description": "JSON Parser",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "tsc": "tsc",
    "test": "tsc && jest --verbose",
    "prepack": "npm install && tsc"
  },
  "files": [
    "dist/index.js",
    "dist/index.d.ts",
    "dist/index.js.map",
    "dist/index.d.ts.map",
    "src/index.ts"
  ],
  "jest": {
    "testEnvironment": "node",
    "testMatch": [
      "**/dist/test/*Tests.js"
    ],
    "reporters": [
      "jest-junit",
      "default"
    ],
    "collectCoverage": true,
    "coverageThreshold": {
      "global": {
        "branches": 93.18,
        "functions": 97.62,
        "lines": 99.47,
        "statements": 98.97
      }
    },
    "coveragePathIgnorePatterns": [
      "/dist/test/"
    ],
    "coverageReporters": [
      "cobertura",
      "text",
      "html"
    ]
  },
  "jest-junit": {
    "outputDirectory": ".",
    "outputName": "test-results.xml"
  },
  "keywords": [
    "json",
    "parser"
  ],
  "author": "Sergey Shandar",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/ts-common/json-parser/issues"
  },
  "homepage": "https://github.com/ts-common/json-parser#readme",
  "devDependencies": {
    "@types/jest": "^24.0.13",
    "@types/node": "^10.12.18",
    "jest": "^24.8.0",
    "jest-junit": "^6.4.0",
    "tslint": "^5.20.1",
    "typescript": "3.5.1"
  },
  "dependencies": {
    "@ts-common/add-position": "0.0.2",
    "@ts-common/iterator": "^0.3.5",
    "@ts-common/json": "^0.3.1",
    "@ts-common/source-map": "^0.5.0",
    "@ts-common/string-map": "^0.3.0",
    "tslib": "^1.9.3"
  }
}
